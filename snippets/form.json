{
  "el-checkbox": {
    "prefix": "el-checkbox",
    "body": [
      " <el-checkbox v-model=\"${1:checked}\">${2:备选项}</el-checkbox>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/checkbox"
  },
  "el-checkbox with disabled": {
    "prefix": "el-checkbox with disabled",
    "body": [
      " <el-checkbox v-model=\"${1:checked}\" ${2:disabled}>${3:备选项}</el-checkbox>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/checkbox"
  },
  "el-checkbox-group": {
    "prefix": "el-checkbox-group",
    "body": [
      "<el-checkbox-group v-model=\"${1:checkList}\">",
      "\t <el-checkbox label=\"${2:复选框 A}\"></el-checkbox>",
      "</el-checkbox-group>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/checkbox"
  },
  "el-input-number": {
    "prefix": "el-input-number",
    "body": [
      "<el-input-number v-model=\"${1:num1}\" @change=\"${2:handleChange}\" :min=\"${3:1}\" :max=\"${4:10}\" label=\"${5:描述文字}\"></el-input-number>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/input-number"
  },
  "el-cascader": {
    "prefix": "el-cascader",
    "body": [
      "<el-cascader",
      "\t:options=\"${1:options}\"",
      "\tv-model=\"${2:selectedOptions}\"",
      "\t@change=\"${3:handleChange}\">",
      "</el-cascader>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/cascader"
  },
  "el-cascader with hover": {
    "prefix": "el-cascader with hover",
    "body": [
      "<el-cascader",
      "\texpand-trigger=\"${1:hover}\"",
      "\t:options=\"${2:options}\"",
      "\tv-model=\"${3:selectedOptions}\"",
      "\t@change=\"${4:handleChange}\">",
      "</el-cascader>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/cascader"
  },
  "el-slider": {
    "prefix": "el-slider",
    "body": [
      "<el-slider v-model=\"${1:value1}\"></el-slider>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/slider"
  },
  "el-slider with disabled": {
    "prefix": "el-slider with disabled",
    "body": [
      "<el-slider v-model=\"${1:value1}\" ${2:disabled}></el-slider>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/slider"
  },
  "el-upload": {
    "prefix": "el-upload",
    "body": [
      "<el-upload",
      "\tclass=\"upload-demo\"",
      "\taction=\"${1:https://jsonplaceholder.typicode.com/posts/}\"",
      "\t:on-preview=\"${2:handlePreview}\"",
      "\t:on-remove=\"${3:handleRemove}\"",
      "\t:before-remove=\"${4:beforeRemove}\"",
      "\t${5:multiple}",
      "\t:limit=\"${6:3}\"",
      "\t:on-exceed=\"${7:handleExceed}\"",
      "\t:file-list=\"${8:fileList}\">",
      "\t<el-button size=\"small\" functional>上传</el-button>",
      "\t<div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>",
      "</el-upload>"
    ],
    "description": "http://ui.shop.jd.com/docs/element-ui/#/zh-CN/component/upload"
  }
}
